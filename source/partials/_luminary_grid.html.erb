<div class="container">
  <div class="row">
    <% luminaries.each do |luminary| %>
    <% url = "/images/luminaries/" + CGI.escape(luminary).gsub("+"," ") %>
    <% name = luminary.slice(0..-5) %>
      <div class="col-lg-3">
        <img src="<%= url %>" class="img-thumbnail">
        <p class="lumheading center"><%= name %></p>
      </div>
    <% end %>
  </div>
</div>
